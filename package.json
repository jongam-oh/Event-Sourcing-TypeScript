{
  "name": "event-sourcing-typescript",
  "version": "1.0.0",
  "description": "Event Sourcing example using TypeScript",
  "main": "lib/app",
  "typings": "lib/app",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build:live",
    "start:models": "cross-env NODE_ENV=development nodemon ./src/bin/www-models",
    "start:commands": "cross-env NODE_ENV=development nodemon ./src/bin/www-commands",
    "build:live": "nodemon ./lib/app.js",
    "precompile": "npm run clean",
    "clean": "rimraf lib/*",
    "compile": "npm run lint && npm run compile:ts",
    "compile:watch": "npm run compile:ts -- -w",
    "compile:ts": "tsc --outDir ./lib --project tsconfig.json",
    "lint": "npm run lint:ts",
    "lint:ts": "tslint src/**/*.ts"
  },
