{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": [
      false, "log"
    ],
    "quotemark": [true, "single", "avoid-escape"],
    "max-classes-per-file": [
      false
    ],
    "interface-name": [
      false
    ],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
